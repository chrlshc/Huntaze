# Bright Data proxy for DM sending
BRIGHT_DATA_CUSTOMER_ID=brd-customer-xxxx
BRIGHT_DATA_PASSWORD=your-brightdata-password
BRIGHT_DATA_HOST=brd.superproxy.io
BRIGHT_DATA_PORT=22225
# Optional sticky options
BRIGHT_DATA_COUNTRY=us

# AWS Region (Bedrock)
AWS_REGION=us-west-1
AWS_DEFAULT_REGION=us-west-1
BEDROCK_REGION=us-west-2

# Bedrock model configuration
# Default: Anthropic Claude 3 Haiku on Bedrock
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20240620-v1:0

# Use Bedrock AI for all DM stages (initial, follow-up)
USE_BEDROCK_AI_DM=true

# Instagram accounts JSON path (if applicable)
# IG_ACCOUNTS_FILE=./instagram-accounts.json
