# AWS Credentials for Database Migration
# Copy this file to .env.migration and fill in your actual credentials
# DO NOT commit .env.migration to version control!

# AWS Access Credentials
export AWS_ACCESS_KEY_ID="your-aws-access-key-id"
export AWS_SECRET_ACCESS_KEY="your-aws-secret-access-key"
export AWS_SESSION_TOKEN="your-aws-session-token"

# Database Configuration
export DB_HOST="huntaze-postgres-production.c2ryoow8c5m4.us-east-1.rds.amazonaws.com"
export DB_PORT="5432"
export DB_NAME="postgres"
export DB_USER="huntazeadmin"
export DB_PASSWORD="your-database-password"

# Full DATABASE_URL (will be constructed from above)
export DATABASE_URL="postgresql://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/${DB_NAME}"
