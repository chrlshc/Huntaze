    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "5.90.5",
    "@trpc/client": "11.6.0",
    "@trpc/next": "11.6.0",
    "@trpc/react-query": "11.6.0",
    "@trpc/server": "11.6.0",
    "@types/nodemailer": "^7.0.2",
    "@types/pg": "^8.15.5",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.6",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "ioredis": "^5.8.2",
    "jose": "^6.1.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.546.0",
    "next": "^15.1.0",
    "nodemailer": "7.0.10",
    "openai": "6.7.0",
    "p-queue": "^9.0.0",
    "pg": "^8.16.3",
    "prisma": "6.18.0",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^8.1.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.0.0",
    "react-hook-form": "7.65.0",
    "react-intersection-observer": "^9.16.0",
    "stripe": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "typescript": "5.9.3",
    "web-vitals": "^3.5.0",
    "zod": "4.1.12",
    "zustand": "5.0.8"
