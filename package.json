    "three:rollback:force": "node scripts/rollback-three-deps.js --force",
    "three:monitor": "node -e \"console.log('Three.js monitoring is active. Check /api/monitoring/three-js-errors for stats.')\"",
    "amplify:validate": "node scripts/validate-amplify-build.js",
    "staging:diagnose": "node scripts/staging-login-cli.js --diagnose",
    "staging:validate": "node scripts/staging-login-cli.js --validate",
    "staging:fix": "node scripts/staging-login-cli.js --fix",
    "staging:rollback": "node scripts/staging-login-cli.js --rollback",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit && tsc -p tsconfig.typecheck.api-proactive.json --noEmit",
    "typecheck:perf": "tsc -p lib/smart-onboarding/performance/tsconfig.perf.json --noEmit",
    "build:perf": "tsc -p lib/smart-onboarding/performance/tsconfig.perf.json"
