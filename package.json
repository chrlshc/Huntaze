{
  "name": "huntaze",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --webpack",
    "postinstall": "node scripts/patch-next-exports.js || true",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:unit:optimized": "NODE_OPTIONS='--max-old-space-size=2048' vitest run tests/unit --reporter=basic --no-coverage",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:integration:optimized": "NODE_OPTIONS='--max-old-space-size=2048' vitest run --config vitest.config.integration.ts --reporter=basic --no-coverage",
    "test:beta": "NODE_OPTIONS='--max-old-space-size=1024' vitest run --config vitest.config.beta.ts --reporter=default",
    "test:beta:watch": "NODE_OPTIONS='--max-old-space-size=1024' vitest watch --config vitest.config.beta.ts --reporter=default",
    "test:e2e": "vitest run --config vitest.config.e2e.ts",
    "test:e2e:watch": "vitest watch --config vitest.config.e2e.ts",
    "test:integration:revenue": "vitest run tests/integration/revenue --config vitest.config.integration.ts",
    "test:integration:watch": "vitest watch --config vitest.config.integration.ts",
    "test:coverage": "vitest run --coverage",
    "test:integration:coverage": "vitest run --coverage --config vitest.config.integration.ts",
    "test:load": "k6 run tests/load/scenarios/baseline.js",
    "test:load:rate-limiter": "k6 run tests/load/rate-limiting/rate-limiter-validation.js",
    "test:load:circuit-breaker": "k6 run tests/load/rate-limiting/circuit-breaker.js",
