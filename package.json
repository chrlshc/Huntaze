    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.1.0",
    "glob": "^10.3.0",
    "jest-axe": "^8.0.0",
    "jsdom": "^23.2.0",
    "playwright": "1.56.1",
    "prettier": "3.6.2",
    "tsx": "^4.7.0",
    "vitest": "4.0.3",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
