    "oauth:report": "tsx scripts/validate-oauth-credentials.ts report",
    "oauth:ready": "tsx scripts/validate-oauth-credentials.ts ready",
    "setup:production": "tsx scripts/setup-production-environment.ts",
    "setup:production:help": "tsx scripts/setup-production-environment.ts --help",
    "layouts:analyze": "tsx scripts/layout-cleanup/analyze-layouts.ts",
    "layouts:cleanup": "tsx scripts/layout-cleanup/cleanup-layouts.ts",
    "layouts:cleanup:dry-run": "tsx scripts/layout-cleanup/cleanup-layouts.ts --dry-run",
    "layouts:stats": "tsx scripts/layout-cleanup/stats-dashboard.ts",
    "layouts:health": "tsx scripts/layout-cleanup/monitor-health.ts",
