    "build:validate": "tsx scripts/layout-cleanup/validate-build.ts",
    "hooks:install": "husky install && chmod +x .husky/pre-commit",
    "amplify:verify-env": "tsx scripts/verify-amplify-env.ts",
    "amplify:setup": "tsx scripts/setup-amplify-deployment.ts",
    "amplify:summary": "./scripts/show-deployment-summary.sh",
    "hooks:log-bypass": "tsx scripts/layout-cleanup/log-bypass.ts",
    "upload-assets": "tsx scripts/upload-assets.ts",
    "upload-assets:dry-run": "tsx scripts/upload-assets.ts --dry-run",
    "deploy:s3-bucket": "bash scripts/deploy-s3-bucket.sh",
    "setup:cloudwatch": "tsx scripts/setup-cloudwatch.ts",
    "test:cloudwatch": "tsx scripts/test-cloudwatch.ts",
    "test:s3-setup": "tsx scripts/test-s3-setup.ts",
    "lighthouse": "bash scripts/run-lighthouse.sh",
    "lighthouse:check": "bash scripts/check-lighthouse-readiness.sh",
    "lighthouse:ci": "lhci autorun",
    "cleanup": "bash scripts/cleanup-disk-space.sh",
    "analyze:disk": "bash scripts/analyze-disk-usage.sh",
    "check:memory": "bash scripts/check-memory.sh",
    "ai:verify": "tsx scripts/verify-ai-setup.ts",
    "migration:status": "tsx scripts/migration-tracker.ts status",
    "migration:next": "tsx scripts/migration-tracker.ts next",
    "migration:report": "tsx scripts/migration-tracker.ts report",
    "migration:list": "tsx scripts/migration-tracker.ts list",
    "migration:update": "tsx scripts/migration-tracker.ts update",
    "cleanup:legacy-styles": "tsx scripts/cleanup-legacy-styles.ts"
