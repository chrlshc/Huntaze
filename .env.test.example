# Test Environment Variables
# Used for running tests
# Copy this to .env.test and fill in real values

# NextAuth
NEXTAUTH_SECRET=test-secret-key-for-testing-only-do-not-use-in-production
NEXTAUTH_URL=http://localhost:3000

# Database (Use your test database URL)
DATABASE_URL="postgresql://username:password@host:5432/database"

# AWS ElastiCache Redis
ELASTICACHE_REDIS_HOST=your-redis-host.cache.amazonaws.com
ELASTICACHE_REDIS_PORT=6379

# API Keys (Mock - not real)
OPENAI_API_KEY=sk-test-key
ANTHROPIC_API_KEY=sk-ant-test-key
GEMINI_API_KEY=test-gemini-api-key-for-testing-only

# Social Media (Mock)
INSTAGRAM_CLIENT_ID=test-instagram-client-id
INSTAGRAM_CLIENT_SECRET=test-instagram-client-secret
NEXT_PUBLIC_INSTAGRAM_REDIRECT_URI=http://localhost:3000/api/auth/callback/instagram

TIKTOK_CLIENT_KEY=test-tiktok-client-key
TIKTOK_CLIENT_SECRET=test-tiktok-client-secret
NEXT_PUBLIC_TIKTOK_REDIRECT_URI=http://localhost:3000/api/auth/callback/tiktok

REDDIT_CLIENT_ID=test-reddit-client-id
REDDIT_CLIENT_SECRET=test-reddit-client-secret
NEXT_PUBLIC_REDDIT_REDIRECT_URI=http://localhost:3000/api/auth/callback/reddit

# OnlyFans (Mock)
ONLYFANS_API_KEY=test-onlyfans-api-key

# Feature Flags
ENABLE_RATE_LIMITING=true
ENABLE_CACHING=true

# Test Mode
NODE_ENV=test
TEST_MODE=true
TOKEN_ENCRYPTION_KEY=test-encryption-key-32-characters-long-for-aes256

# AWS Configuration (Mock credentials for testing)
# NOTE: These are fake credentials for testing only. Use real credentials from AWS SSO when running actual S3 tests.
AWS_REGION=us-east-1
AWS_S3_BUCKET=huntaze-beta-assets
AWS_ACCESS_KEY_ID=REDACTED
AWS_SECRET_ACCESS_KEY=REDACTED
AWS_SESSION_TOKEN=REDACTED-session-token-for-testing-only-not-real

# CloudWatch Configuration (for monitoring tests)
CLOUDWATCH_LOG_GROUP=/aws/huntaze/test
CLOUDWATCH_NAMESPACE=Huntaze/Test
