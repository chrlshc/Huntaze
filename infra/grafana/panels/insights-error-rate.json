{
  "type": "timeseries",
  "title": "Insights Errors/s by platform/kind",
  "targets": [
    {
      "refId": "A",
      "expr": "sum by (platform, kind) (rate(social_insights_fetch_total{status=\"error\"}[$__rate_interval]))",
      "legendFormat": "{{platform}} / {{kind}}"
    }
  ],
  "fieldConfig": {
    "defaults": { "unit": "reqps" }
  },
  "options": {
    "legend": { "displayMode": "list", "placement": "bottom" },
    "tooltip": { "mode": "single" }
  },
  "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 }
}

