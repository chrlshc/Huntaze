{
  "type": "timeseries",
  "title": "Insights Latency p95/p99 by Platform/Kind",
  "targets": [
    {
      "refId": "A",
      "expr": "histogram_quantile(0.95, sum by (platform, kind, le) (rate(social_insights_fetch_latency_seconds_bucket[15m])))",
      "legendFormat": "p95 {{platform}} / {{kind}}"
    },
    {
      "refId": "B",
      "expr": "histogram_quantile(0.99, sum by (platform, kind, le) (rate(social_insights_fetch_latency_seconds_bucket[15m])))",
      "legendFormat": "p99 {{platform}} / {{kind}}"
    }
  ],
  "options": {
    "legend": { "displayMode": "list", "placement": "bottom" },
    "tooltip": { "mode": "single" }
  },
  "gridPos": { "h": 12, "w": 24, "x": 0, "y": 0 }
}

