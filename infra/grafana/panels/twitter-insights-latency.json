{
  "type": "timeseries",
  "title": "Twitter Insights Latency (p95 by kind, p99 global)",
  "targets": [
    {
      "refId": "A",
      "expr": "histogram_quantile(0.95, sum by (kind, le) (rate(social_insights_fetch_latency_seconds_bucket{platform=\"twitter\"}[5m])))",
      "legendFormat": "p95 {{kind}}"
    },
    {
      "refId": "B",
      "expr": "histogram_quantile(0.99, sum by (le) (rate(social_insights_fetch_latency_seconds_bucket{platform=\"twitter\"}[5m])))",
      "legendFormat": "p99 global"
    }
  ],
  "options": {
    "legend": { "displayMode": "list", "placement": "bottom" },
    "tooltip": { "mode": "single" }
  },
  "gridPos": { "h": 10, "w": 24, "x": 0, "y": 0 }
}

