      "Resource": [
        "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/huntaze-oauth-tokens",
        "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/huntaze-oauth-tokens/index/byExpiry"
      ]
    }
  ]
}

